{
	"name": "moonwalkingbits/wordpress-templating",
	"description": "A general purpose templating abstraction.",
	"type": "library",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Martin Pettersson",
			"email": "martin@moonwalkingbits.com"
		}
	],
	"minimum-stability": "stable",
	"scripts": {
		"lint": "phpcs -p --standard=WordPress src/",
		"analyze": [
			"phpmd --suffixes php --exclude vendor/ ./ ansi phpmd.xml",
			"phan --output-mode text --no-progress-bar --color --allow-polyfill-parser"
		]
	},
	"autoload": {
		"wordpress": {
			"Moonwalking_Bits\\Templating\\": "src/"
		},
		"files": [
			"vendor/wordpress-autoload.php"
		]
	},
	"require": {
		"php": ">=7.4",
		"moonwalkingbits/wordpress-autoloader-composer-plugin": "^0.2.2"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.6",
		"wp-coding-standards/wpcs": "^2.3",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"phpmd/phpmd": "^2.10",
		"phan/phan": "^5.3"
	}
}
